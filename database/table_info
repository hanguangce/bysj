product
{
id (key)  int  
ic   内码 int
oc 原厂条形码  int
pp	品牌  string
pm	品名  
kh	 款号  
ty	类别	
jx	经销商 
pn0	电话
p0	手机
prod	制造商
pn1	电话
ph1	手机
}

rep	仓库表
{
ic	内码  (key)  int
ct	库存数量    int
ct0	已销售数量	int
mg	经办人	
}

prices	价格表
{
ic	内码 (key)  int
jp	进价   int
bp	标价	int
lr	利润	int
}

emtpys	员工信息表
{
eid	员工编码  (key) int
ename	姓名
esex	性别
eage	年龄
emon	职位工资
eages	工龄
ecot1	联系方式1
ecot2	联系方式2
ecp	联系人
ecpn	联系人联系方式
}


xsjl	销售记录
{
jylsh	交易流水号 int 
syy	收银员  int 
xsyg	销售员工 int
jysj	交易时间 
jysl	交易数量 int
jyje	交易金额 int 
lr	利润    int
p1	商品1
p2	商品2
p3	商品3
p4	商品4
p5	商品5
p6	商品6
p7	商品7
p8	商品8
p9	商品9
